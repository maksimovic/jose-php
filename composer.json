{
  "name": "maksimovic/jose-php",
  "description": "JWT, JWS and JWS implementation in PHP",
  "keywords": [
    "JSON Web Token", "JSON Web Signature", "JSON Web Encryption",
    "JWT", "JWS", "JWE", "JOSE", "OpenID Connect", "ID Token"
  ],
  "homepage": "https://github.com/maksimovic/jose-php",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Nov Matake",
      "email": "nov@matake.jp",
      "homepage": "http://matake.jp"
    },
    {
      "name": "Oliver Maksimovic",
      "email": "maksimovic@outlook.com",
      "homepage": "https://github.com/maksimovic"
    }
  ],
  "require": {
    "php": "^7.2 | ^8.0",
    "phpseclib/phpseclib": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 | ^9.5"
  },
  "include-path": ["src/"],
  "autoload": {
    "psr-0": {
      "JOSE": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\" : "test/"
    }
  }
}